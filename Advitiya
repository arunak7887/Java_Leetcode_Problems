Code:

import java.util.*;
import java.lang.*;
import java.io.*;
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		if(a==16 || a==17 || a==18){
		    System.out.println("ADVITIYA");
		 
		}else{
		    System.out.println("WAITING FOR ADVITIYA");
		}
	}
}
